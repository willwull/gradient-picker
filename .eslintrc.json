{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "no-underscore-dangle": 0,
        "no-console": 0,
        "prefer-destructuring": 0,
        "no-plusplus": 0,
        "react/jsx-filename-extension": 0
    }
}